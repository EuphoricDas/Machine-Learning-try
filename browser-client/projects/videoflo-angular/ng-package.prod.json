  "dest": "../../dist/videoflo-angular",
  "assets": ["index.css", "index.minimal.css", "assets"],
  "lib": {
    "entryFile": "src/public_api.ts",
    "umdModuleIds": {
      "openvidu-browser": "openvidu-browser",
      "linkifyjs": "linkifyjs",
      "ngx-socket-io": "ngx-socket-io",
